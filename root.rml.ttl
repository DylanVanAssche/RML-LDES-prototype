@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rmlt: <http://semweb.mmlab.be/ns/rml-target#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix formats: <http://www.w3.org/ns/formats/> .
@prefix tree: <https://w3id.org/tree#> .

@base <http://example.com/base/> .

<#TargetDump> a rmlt:LogicalTarget;
  rmlt:target [ a void:Dataset;
    void:dataDump <file://./public/root.ttl>;
  ];
  rmlt:serialization formats:Turtle;
.

<TriplesMap1>
  a rr:TriplesMap;
    
  rml:logicalSource [ 
    rml:source "list.csv";
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [ 
    rr:template "http://pieter.pm/RML-LDES-prototype/root.ttl";
    rml:logicalTarget <#TargetDump>;
  ]; 
	
  rr:predicateObjectMap [ 
    rr:predicate tree:member; 
    rr:objectMap [
      rr:template "https://pieter.pm/RML-LDES-prototype/{file}";
      rr:termType rr:IRI;
    ]
  ].
